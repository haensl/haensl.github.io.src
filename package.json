{
  "name": "haensl.github.io",
  "version": "2.21.0",
  "description": "The github.io page of HP Dietz.",
  "main": "dist/index.html",
  "engines": {
    "node": ">= 6.9.2"
  },
  "scripts": {
    "watch:client": "gulp watch",
    "start:server": "cd dist/hpdietz.com && pm2-runtime start pm2.json",
    "start": "[ \"$NODE_ENV\" == 'production' ] && pm2-runtime start pm2.json || run-s build:src dev",
    "dev:haensl.github.io": "gulp serve",
    "dev:hpdietz.com": "run-p watch:client start:server",
    "dev": "run-p dev:haensl.github.io dev:hpdietz.com",
    "build:src": "gulp build",
    "build:docker": "docker build -t haensl.github.io:2.20.1 .",
    "build": "run-s build:src build:docker",
    "deploy:qa": "docker save haensl.github.io | bzip2 | pv | ssh $VMSERVER_USER@$VMSERVER 'bunzip2 | docker load'; dvmserver run -d -p 8081:8081 --name haensl.github.io --network=haensl-github-io haensl.github.io:latest",
    "qa": "run-s build deploy:qa",
    "publish:hpdietz": "bash deploy-hpdietz.com"
  },
  "keywords": [
    "github.io",
    "github",
    "pages",
    "personal",
    "website",
    "amp",
    "amp-html",
    "accelerated mobile pages"
  ],
  "author": {
    "name": "HP Dietz",
    "email": "h.p.dietz@gmail.com",
    "url": "https://haensl.github.io"
  },
  "license": "GPL-3.0",
  "bugs": "https://github.com/haensl/haensl.github.io.src/issues",
  "homepage": "https://haensl.github.io",
  "repository": "https://github.com/haensl/haensl.github.io.src/haensl.github.io.src.git",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "browser-sync": "^2.23.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-amphtml-validator": "^1.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-embed-json": "^1.2.0",
    "gulp-embed-svg": "^1.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-insert": "^0.5.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-mustache": "^3.0.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "pm2": "^2.9.1"
  }
}
