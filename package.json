    "node": ">= 6.9.2"
  },
  "scripts": {
    "watch:client": "gulp watch",
    "start:server": "cd dist/hpdietz.com && node app.js",
    "start": "[ \"$NODE_ENV\" == 'production' ] && node app.js || run-s build:src dev",
    "dev:haensl.github.io": "gulp serve",
    "dev:hpdietz.com": "run-p watch:client start:server",
    "dev": "run-p dev:haensl.github.io dev:hpdietz.com",
    "build:src": "gulp build",
    "build:docker": "docker-compose build",
    "build": "run-s build:src build:docker",
    "dist:github": "cd dist/haensl.github.io && git push origin master"
  },
  "keywords": [
