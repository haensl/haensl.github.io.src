{
  "name": "haensl.github.io",
  "version": "2.11.1",
  "description": "The github.io page of HP Dietz.",
  "main": "dist/index.html",
  "engines": {
    "node": ">= 6.9.2"
  },
  "scripts": {
    "watch:client": "gulp",
    "watch:server": "pm2-runtime start dist/app.js --watch",
    "start": "[ \"${NODE_ENV}\" != 'production' ] && run-p watch:* || pm2-runtime start app.js",
    "build": "gulp build",
    "build:docker": "docker build -t haensl.github.io .",
    "compile": "run-p build build:docker",
    "deploy:qa": "docker save haensl.github.io | bzip2 | pv | ssh $VMSERVER_USER@$VMSERVER 'bunzip2 | docker load'; dvmserver run -d -p 8081:8081 --rm --name haensl.github.io --network=haensl-github-io haensl.github.io:latest",
    "qa": "run-s compile deploy:qa"
  },
  "keywords": [
    "github.io",
    "github",
    "pages",
    "personal",
    "website",
    "amp",
    "amp-html",
    "accelerated mobile pages"
  ],
  "author": {
    "name": "HP Dietz",
    "email": "h.p.dietz@gmail.com",
    "url": "https://haensl.github.io"
  },
  "license": "GPL-3.0",
  "bugs": "https://github.com/haensl/haensl.github.io.src/issues",
  "homepage": "https://haensl.github.io",
  "repository": "https://github.com/haensl/haensl.github.io.src/haensl.github.io.src.git",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "browser-sync": "^2.23.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-amphtml-validator": "^1.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-embed-json": "^1.2.0",
    "gulp-embed-svg": "^1.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-mustache": "^3.0.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "pm2": "^2.9.1"
  }
}
