{
  "vars": {
    "account": "UA-109682541-1"
  },
  "triggers": {
    "trackPageView": {
      "on": "visible",
      "request": "pageview"
    },
    "trackSectionView": {
      "on": "click",
      "selector": "#main-menu a",
      "request": "event",
      "vars": {
        "eventCategory": "Pageview",
        "eventAction": "view",
        "eventLabel": "${section}"
      }
    }
    "trackSocialView": {
      "on": "click",
      "selector": ".social-links a",
      "request": "social",
      "vars": {
        "socialNetwork": "${network}",
        "socialAction": "view"
      }
    },
    "trackDocView": {
      "on": "click",
      "selector": ".doc-link",
      "request": "event",
      "vars": {
        "eventCategory": "DocumentAccess",
        "eventAction": "view",
        "eventLabel": "${document}"
      }
    },
    "trackProject": {
      "on": "click",
      "selector": ".project-link",
      "request": "event",
      "vars": {
        "eventCategory": "Projectview",
        "eventAction": "view",
        "eventLabel": "${project}"
      }
    }
  }
}
